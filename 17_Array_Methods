let num=[1,2,3,4,5,6,7,8]
console.log(typeof num)

let b=num.toString()
console.log(b,"   ",typeof b)

let c=num.join(" and ")
console.log(c,"   ",typeof c)

num.pop()
console.log(num)

let r=num.pop() //pop returns the popped element
console.log(num,"   ",r)

let s=num.push(56) // push returns the new array length
console.log(num,"   ",s)

let q=num.shift() //remove the first element ans returns ans
console.log(num,"   ",q)

let t=num.unshift(78) //adds the element in the beginning and return the new array length
console.log(num,"   ",t)
